{
    "name": "attla/authentic",
    "description": "An effective stateless user authentication and authorization layer.",
    "keywords": [
        "attla",
        "laravel",
        "auth",
        "authn",
        "authz",
        "stateless",
        "stateful",
        "serverless",
        "authentic",
        "authentication",
        "user-authentication",
        "serverless-authentication",
        "cookier-authentication"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolau",
            "email": "nicolau@zunq.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "attla/cookier": "dev-main",
        "attla/token": "dev-main",
        "attla/support": "dev-main",
        "illuminate/auth": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/cookie": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/events": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/validation": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Attla\\Authentic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Attla\\Authentic\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
